<script type="text/x-handlebars-template" id="grouppedaccounts-related">
    
  <div class="panel panel-default">
    <div class="panel-heading">
      <h3 class="panel-title">Accounts related  
          {{#if groupname }}
            to group :{{ groupname }}
          {{/if}}
      </h3>
    </div>
    <div class="panel-body">
      <table class="table table-striped table-bordered table-list" id='table-related'>
        <tbody class="connectedSortable">
          <tr>
            <th>Network</th>
            <th>Accounts</th>
          </tr> 
          {{#each groupped}}
          {{#each related}}
          <tr>
            <td>
            {{social_account_url.bucket}}
            </td>
            <td> 
            {{social_account_url.username}}
            </td>  
          </tr>
          {{/each}}
          {{/each}}
        </tbody>
      </table>
    </div>
  </div>
  
</script>