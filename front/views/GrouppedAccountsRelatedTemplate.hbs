<script type="text/x-handlebars-template" id="grouppedaccount-a">

  <div class="row">
    <div class="col-xs-6">      
      <div class="panel panel-default">
        <div class="panel-heading">
          <h3 class="panel-title">Accounts Related</h3>
        </div>
        <div class="panel-body">
          <table class="table table-striped table-bordered table-list">
            <thead>
            <tr>
            <th>Group</th>
            <th>Network</th>
            <th>Accounts</th>
            </tr> 
            </thead>
              <tbody>
              {{#each groupped}}
              {{#each related}}
                <tr>
              <td>  
              {{#with (lookup2 ../../groups social_group) }}
              {{ name }}
              {{/with}}
              </td>
              <td>
              {{social_account_url.bucket}}
              </td>
              <td> 
              {{social_account_url.username}}
              </td>  
                </tr>
              {{/each}}
              {{/each}}
             </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
</script>